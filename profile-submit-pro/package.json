{
  "name": "profile-submit-pro",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel watch assets/js/*.js --dist-dir assets/js/dist",
    "build": "parcel build assets/js/*.js --dist-dir assets/js/dist",
    "css:dev": "tailwindcss -i ./assets/css/profile-submit-pro.css -o ./assets/css/dist/profile-submit-pro.css --watch",
    "css:build": "tailwindcss -i ./assets/css/profile-submit-pro.css -o ./assets/css/dist/profile-submit-pro.css --minify"
  },
  "dependencies": {
    "alpinejs": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  }
}
