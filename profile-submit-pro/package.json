    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.47",
    "postcss-custom-properties": "^14.0.4",
    "postcss-url": "^10.1.3",
    "prettier": "3.3.3",
